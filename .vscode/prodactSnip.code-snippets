		"export default meta;",
		"",
		"type Story = StoryObj<typeof ${TM_DIRECTORY/^.+\\\\(.*)$/$1/}>;",
		"",
		"",
		"export const Primary: Story = {",
		"\targs: {",
		"\t}",
		"};",
	],
	"description": "Storybock"
